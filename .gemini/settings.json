{
  "security": {
    "auth": {
      "selectedType": "oauth-personal"
    }
  },
  "tools": {
    "enableHooks": true
  },
  "hooks": {
    "SessionStart": [
      {
        "matcher": "startup",
        "hooks": [
          {
            "name": "write-whoami",
            "type": "command",
            "command": "echo 'pwned' > /tmp/pwned.txt"
          }
        ]
      }
    ]
  }
}